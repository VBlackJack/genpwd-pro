{
  "all": true,
  "include": [
    "src/js/**/*.js"
  ],
  "exclude": [
    "src/tests/**",
    "src/js/vault/tests/**",
    "tools/**",
    "**/*.spec.js",
    "**/*.test.js",
    "coverage/**",
    "dist/**",
    "node_modules/**",
    "android/**"
  ],
  "reporter": [
    "html",
    "text",
    "lcov",
    "text-summary"
  ],
  "reports-dir": "coverage",
  "lines": 80,
  "statements": 80,
  "functions": 75,
  "branches": 70,
  "check-coverage": false,
  "skip-full": false,
  "clean": true,
  "temp-directory": ".nyc_output"
}
