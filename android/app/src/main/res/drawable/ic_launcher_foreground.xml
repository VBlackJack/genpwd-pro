<?xml version="1.0" encoding="utf-8"?>
<!--
    GenPwd Pro - Launcher Icon Foreground
    Design: Key + Lock symbol representing password generation and security
-->
<vector xmlns:android="http://schemas.android.com/apk/res/android"
    android:width="108dp"
    android:height="108dp"
    android:viewportWidth="108"
    android:viewportHeight="108">

    <!-- Safe area: 66dp circle centered at 54,54 -->
    <group
        android:translateX="54"
        android:translateY="54">

        <!-- Lock body (main element) -->
        <path
            android:fillColor="#FFFFFF"
            android:pathData="M-12,-4 L12,-4 Q16,-4 16,0 L16,12 Q16,16 12,16 L-12,16 Q-16,16 -16,12 L-16,0 Q-16,-4 -12,-4 Z" />

        <!-- Lock shackle (arc on top) -->
        <path
            android:strokeColor="#FFFFFF"
            android:strokeWidth="3"
            android:strokeLineCap="round"
            android:fillColor="#00000000"
            android:pathData="M-8,-4 Q-8,-14 0,-14 Q8,-14 8,-4" />

        <!-- Key symbol inside lock -->
        <group
            android:translateY="2">
            <!-- Key shaft -->
            <path
                android:fillColor="#4CAF50"
                android:pathData="M-4,2 L4,2 L4,8 L-4,8 Z" />

            <!-- Key head (circle) -->
            <path
                android:fillColor="#4CAF50"
                android:pathData="M0,-2 m-4,0 a4,4 0,1,0 8,0 a4,4 0,1,0 -8,0" />

            <!-- Key teeth -->
            <path
                android:fillColor="#4CAF50"
                android:pathData="M4,6 L6,6 L6,8 L4,8 Z M4,3 L7,3 L7,5 L4,5 Z" />
        </group>

        <!-- Shine effect (top-left highlight) -->
        <path
            android:fillColor="#FFFFFF"
            android:fillAlpha="0.3"
            android:pathData="M-8,-2 Q-6,-8 -2,-6 L-4,-2 Z" />

        <!-- Password dots decoration -->
        <group
            android:translateY="18">
            <circle
                android:fillColor="#2196F3"
                android:fillAlpha="0.6"
                android:cx="-6"
                android:cy="0"
                android:radius="1.5" />
            <circle
                android:fillColor="#2196F3"
                android:fillAlpha="0.6"
                android:cx="-2"
                android:cy="0"
                android:radius="1.5" />
            <circle
                android:fillColor="#2196F3"
                android:fillAlpha="0.6"
                android:cx="2"
                android:cy="0"
                android:radius="1.5" />
            <circle
                android:fillColor="#2196F3"
                android:fillAlpha="0.6"
                android:cx="6"
                android:cy="0"
                android:radius="1.5" />
        </group>
    </group>
</vector>
